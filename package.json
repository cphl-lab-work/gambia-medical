{
  "name": "hm2-v2",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "db:migrate": "ts-node --project tsconfig.server.json node_modules/typeorm/cli.js migration:run -d src/database/data-source.ts",
    "db:migrate:revert": "ts-node --project tsconfig.server.json node_modules/typeorm/cli.js migration:revert -d src/database/data-source.ts",
    "db:seed": "ts-node --project tsconfig.server.json src/seed/run-seed.ts",
    "typeorm": "ts-node --project tsconfig.server.json node_modules/typeorm/cli.js"
  },
  "dependencies": {
    "next": "14.2.18",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "pg": "^8.13.1",
    "typeorm": "^0.3.20",
    "reflect-metadata": "^0.2.2",
    "bcryptjs": "^2.4.3",
    "uuid": "^11.0.3"
  },
  "devDependencies": {
    "@types/node": "^22.9.0",
    "@types/react": "^18.3.12",
    "@types/react-dom": "^18.3.1",
    "@types/bcryptjs": "^2.4.6",
    "@types/uuid": "^10.0.0",
    "typescript": "^5.6.3",
    "ts-node": "^10.9.2",
    "eslint": "^8.57.1",
    "eslint-config-next": "14.2.18",
    "tailwindcss": "^3.4.14",
    "postcss": "^8.4.47",
    "autoprefixer": "^10.4.20"
  }
}
