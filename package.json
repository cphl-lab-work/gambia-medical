{
  "name": "hm2-v2",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "db:migrate": "ts-node --project tsconfig.server.json -r dotenv/config node_modules/typeorm/cli.js migration:run -d src/database/data-source.ts",
    "db:migrate:revert": "ts-node --project tsconfig.server.json -r dotenv/config node_modules/typeorm/cli.js migration:revert -d src/database/data-source.ts",
    "db:baseline": "ts-node --project tsconfig.server.json scripts/sync-migration-baseline.ts",
    "db:seed": "ts-node --project tsconfig.server.json src/seed/run-seed.ts",
    "db:setup": "npm run db:migrate && npm run db:seed",
    "typeorm": "ts-node --project tsconfig.server.json -r dotenv/config node_modules/typeorm/cli.js"
  },
  "dependencies": {
    "bcryptjs": "^2.4.3",
    "next": "14.2.18",
    "pg": "^8.19.0",
    "react": "^18.3.1",
    "react-datepicker": "^9.1.0",
    "react-dom": "^18.3.1",
    "react-phone-input-2": "^2.15.1",
    "react-select": "^5.10.2",
    "recharts": "^3.7.0",
    "reflect-metadata": "^0.2.2",
    "typeorm": "^0.3.28",
    "uuid": "^11.0.3"
  },
  "devDependencies": {
    "@netlify/plugin-nextjs": "^5.15.8",
    "@types/bcryptjs": "^2.4.6",
    "@types/node": "^22.9.0",
    "@types/react": "^18.3.12",
    "@types/react-dom": "^18.3.1",
    "@types/uuid": "^10.0.0",
    "autoprefixer": "^10.4.20",
    "eslint": "^8.57.1",
    "eslint-config-next": "14.2.18",
    "postcss": "^8.4.47",
    "tailwindcss": "^3.4.14",
    "ts-node": "^10.9.2",
    "typescript": "^5.6.3"
  }
}
